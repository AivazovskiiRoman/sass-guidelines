{
  "scripts": {
    "postinstall": "npm run prejs",
    "build": "bin/build",
    "postbuild": "bin/testbuild",
    "preicons": "svgo assets/images/icons",
    "icons": "spritesh --input assets/images/icons --output _includes/sprite.svg --viewbox '0 0 1024 1024' --prefix icon-",
    "start": "npm run build && npm run watch",
    "watch": "JEKYLL_ENV=LOCAL jekyll s --incremental --config _config.yml,_config.dev.yml",
    "prejs": "cp node_modules/accessible-modal-dialog/accessible-modal-dialog.js assets/js/vendor",
    "js": "rollup --format iife assets/js/src/main.js | uglifyjs > assets/js/main.min.js"
  },
  "devDependencies": {
    "accessible-modal-dialog": "^1.0.1",
    "autoprefixer": "^6.3.3",
    "clean-css": "^3.4.9",
    "criticalcss": "^1.0.1",
    "fg-loadcss": "^0.2.4",
    "postcss-cli": "^2.5.1",
    "rollup": "^0.25.4",
    "spritesh": "^1.0.8",
    "svgo": "^0.6.1",
    "uglify-js": "^2.4.15",
    "uncss": "^0.13.0",
    "woff2-feature-test": "^1.0.2"
  }
}
